/* ========== Tables / stats / hair toggles ========== */

.hairshort,
.hairmed,
.hairlong,
.hairmisc{
  display:none;
  visibility:hidden;
}

.visible{
  display:block;
  visibility:visible;
}

/* Base table styling for all passage tables */
.passage table{
  width:100%;
  border-collapse:collapse;
  table-layout:fixed;
  margin:0 0 16px 0;
  background:transparent;
}

.passage th,
.passage td{
  padding:8px 10px;
  vertical-align:middle;
  border-bottom:1px solid rgba(0,0,0,0.12);
  word-wrap:break-word;
  overflow-wrap:anywhere;
}

/* Label column (th) */
.passage th{
  width:200px;
  text-align:right;
  padding-right:12px;
  font-weight:600;
  color:#999;
}

/* Value column(s) */
.passage td{
  text-align:left;
}

/* Last column */
.passage td:last-child{
  white-space:normal;
  padding-left:10px;
}

/* NSFW table */
table.nsfw{ margin-top:8px; }

/* Subsection headers */
h2, h3{ margin:4px 0; }

/* Section wrapper around stats */
.stats-section{
  margin:16px 0 24px;
  padding:12px;
  border:1px solid rgba(0,0,0,0.08);
  border-radius:10px;
  background:rgba(0,0,0,0.02);
}

@media (prefers-color-scheme:dark){
  .stats-section{
    border-color:rgba(255,255,255,0.14);
    background:rgba(255,255,255,0.04);
  }
}

/* Stats tables */
.passage table.stats-table{
  width:100%;
  border-collapse:collapse;
  table-layout:fixed;
  margin:0 0 12px 0;
  background:transparent;
}

.passage table.stats-table th,
.passage table.stats-table td{
  padding:8px 12px;
  vertical-align:top;
  border-bottom:1px solid rgba(0,0,0,0.10);
  overflow-wrap:anywhere;
}

.passage table.stats-table th{
  text-align:right;
  font-weight:600;
  color:#222;
  opacity:.9;
}

.passage table.stats-table td{
  text-align:left;
  color:#111;
}

/* Zebra striping */
.passage table.stats-table tr:nth-child(odd) td,
.passage table.stats-table tr:nth-child(odd) th{
  background:rgba(0,0,0,0.02);
}

/* Optional third-column actions */
.passage table.stats-table td.action{
  width:1%;
  white-space:nowrap;
  padding-left:8px;
}

/* Completed row: neon highlight for stats table rows */
.passage table.stats-table tr.green th,
.passage table.stats-table tr.green td{
  background:radial-gradient(circle at left, rgba(40, 200, 140, 0.22), transparent);
  border-bottom-color:rgba(80, 255, 180, 0.45);
}

/* Left accent bar on the row itself */
.passage table.stats-table tr.green{
  box-shadow:inset 3px 0 0 rgba(80, 255, 180, 0.85);
}

/* Label cell tweak + checkmark */
.passage table.stats-table tr.green th{
  color:#e6fff5;
}

.passage table.stats-table tr.green th::before{
  content:"✔";
  font-size:0.8em;
  margin-right:0.35em;
  opacity:0.85;
}

@media (prefers-color-scheme:dark){
  .passage table.stats-table th,
  .passage table.stats-table td{
    border-bottom-color:rgba(255,255,255,0.12);
  }
  .passage table.stats-table th{ color:#e6e6e6; }
  .passage table.stats-table td{ color:#f2f2f2; }
  .passage table.stats-table tr:nth-child(odd) td,
  .passage table.stats-table tr:nth-child(odd) th{
    background:rgba(255,255,255,0.05);
  }

  .passage table.stats-table tr.green th,
  .passage table.stats-table tr.green td{
    background:radial-gradient(circle at left, rgba(40, 200, 140, 0.25), transparent);
    border-bottom-color:rgba(80, 255, 180, 0.55);
  }

  .passage table.stats-table tr.green th{
    color:#eafff9;
  }
}

/* Mobile: relax fixed layout */
@media (max-width:640px){
  .passage table.stats-table{ table-layout:auto; }
}

/* ========== Dialogue speaker styles ========== */

.Jinx{
  font-weight:600;
  letter-spacing:0.06em;
  text-shadow:
    0 0 6px rgba(120,180,255,0.9),
    0 0 18px rgba(80,120,255,0.8);
  color:#c9e2ff;
}

.mc{
  font-style:italic;
  color:#ffd8f0;
}

/* UI-NPC-Sex header alignment */
#passage-UI-NPC-Sex .status-row{
  display:flex;
  align-items:center;
  gap:16px 24px;
  flex-wrap:wrap;
}

#passage-UI-NPC-Sex .status-item{
  display:inline-flex;
  align-items:center;
  gap:8px;
}

#passage-UI-NPC-Sex .label{ font-weight:600; }

#passage-UI-NPC-Sex .value{
  display:inline-flex;
  align-items:center;
  gap:8px;
}

#passage-UI-NPC-Sex .stage-pill{
  display:inline-block;
  padding:2px 8px;
  border:1px solid rgba(255,255,255,.25);
  border-radius:8px;
  line-height:1.2;
}

/* Generic status line block (non-UI-NPC-Sex specific) */
.status-line{
  display:grid;
  grid-template-columns:100px auto;
  align-items:center;
}

.status-label{
  font-weight:600;
  text-align:right;
  white-space:nowrap;
}

.status-value{
  text-align:left;
  min-height:1.2em;
}

.index-flex{
  display:flex;
  flex-wrap:wrap;
  gap:1rem;
}

.index-flex > div{
  background:rgba(255,255,255,0.04);
  padding:12px;
  border-radius:10px;
  border:1px solid rgba(255,255,255,0.08);
  min-width:220px;
}

/* Character Creator completed box styling */
.index-flex div.green{
  background:linear-gradient(135deg, rgba(40, 200, 140, 0.25), rgba(0,0,0,0.10));
  border:1px solid rgba(80, 255, 180, 0.55);
  box-shadow:
    inset 0 0 12px rgba(0, 0, 0, 0.6),
    0 0 8px rgba(80, 255, 180, 0.55);
  border-radius:10px;
  padding:12px;
  transition:0.2s ease;
  color:#dfffe9;
  font-weight:600;
}

/* Optional: checkmark before the label */
.index-flex div.green::before{
  content:"✔ ";
  color:#8fffbf;
  font-weight:900;
}
