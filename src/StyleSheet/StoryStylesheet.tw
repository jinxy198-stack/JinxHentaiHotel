  75%  { clip: rect(86px, 9999px, 50px, 0); }
  80%  { clip: rect(2px, 9999px, 46px, 0); }
  85%  { clip: rect(41px, 9999px, 71px, 0); }
  90%  { clip: rect(75px, 9999px, 15px, 0); }
  95%  { clip: rect(41px, 9999px, 8px, 0); }
  100% { clip: rect(23px, 9999px, 75px, 0); }
}

@keyframes noise-animY{
  0%   { clip-path: polygon(0 80px, 100% 80px, 100% 16px, 0 16px); }
