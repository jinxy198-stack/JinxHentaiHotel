:: StoryStylesheet [stylesheet]


.sex-list {
  display: flex;
  flex-wrap: wrap;
  gap: 0.5em;
  padding: 0;
  margin: 0.5em 0;
  list-style: none;
}

.sex-item {
  display: inline-flex;
}

.sex-item a,
.sex-item button {
  padding: 0.4em 0.7em;
  border-radius: 4px;
  background: #222;
  color: #fff;
  text-decoration: none;
  cursor: pointer;
  white-space: nowrap;
}

.sex-item a:hover,
.sex-item button:hover {
  background: #444;
}




/* Make sure the PositionBlock never extends under the sidebar */
.position-block{
  box-sizing:border-box;
  max-width:100%;
  overflow-wrap:break-word;
  word-wrap:break-word;
  white-space:normal;
}

html, body{
  background: radial-gradient(circle at top, #222 0, #050509 55%, #000 100%);
  color:#eee;
}

/* Frame the story area next to the sidebar */
#story{
  max-width:960px;
  margin:1.5rem 1.5rem 1.5rem 21rem;
  padding:1.5rem 2rem;
  background:linear-gradient(135deg, rgba(10,10,20,.96), rgba(5,5,10,.98));
  border-radius:16px;
  box-shadow:
    0 0 0 1px rgba(255,255,255,0.04),
    0 0 40px rgba(0,0,0,0.9);
  text-align:left;
  overflow-x:hidden;
}

/* ---------- UI-NPC-Sex layout (ADD HERE) ---------- */

/* Mobile: when the screen is small, drop the big left margin */
@media (max-width:900px){
  #story{ margin:1rem; }
}

/* Location styling */
.room-hotel{
  border-left:3px solid #ff7be9;
  padding-left:0.75rem;
}

/* NPC card / panels */
.npc-card,
.npcsex-root .npc-panel{
  background:linear-gradient(145deg, rgba(20,20,35,0.98), rgba(8,8,16,0.98));
  border-radius:14px;
  padding:0.75rem 1rem;
  margin-bottom:0.75rem;
  border:1px solid rgba(255,255,255,0.04);
  box-shadow:0 0 18px rgba(0,0,0,0.7);
}

.npc-name{
  font-weight:700;
  letter-spacing:0.08em;
  text-transform:uppercase;
  font-size:0.85rem;
}

.npc-name::before{
  content:"✦ ";
}

/* Global buttons: neon hotel choices */
button{
  border-radius:999px;
  padding:0.35rem 0.95rem;
  font-size:0.9rem;
  text-transform:uppercase;
  letter-spacing:0.08em;
  border:1px solid rgba(255,255,255,0.15);
  background:radial-gradient(circle at top, #3a1480, #170623);
  color:#fff;
  box-shadow:
    0 0 0 1px rgba(255,255,255,0.04),
    0 0 10px rgba(173, 64, 255, 0.4);
  cursor:pointer;
}

button:hover,
html[data-outlines] button:focus{
  background:radial-gradient(circle at top, #5f22c0, #201032);
  box-shadow:0 0 16px rgba(200, 120, 255, 0.7);
  transform:translateY(-1px);
}

button:active{
  transform:translateY(0);
  box-shadow:0 0 4px rgba(120, 60, 180, 0.5);
}

button.active-choice{
  background:radial-gradient(circle at top, #6a28ff, #24063b);
  border-color:rgba(255,255,255,0.35);
  box-shadow:
    inset 0 0 8px rgba(0,0,0,0.9),
    0 0 14px rgba(180, 80, 255, 0.8);
  transform:none;
  filter:none;
}

/* ===== UI-NPC-Sex: non-overlapping layout ===== */

/* Make the UI-NPC-Sex passage a flex column that fills the viewport */
#passage-UI-NPC-Sex{
  display:flex;
  flex-direction:column;
  min-height:calc(100vh - 3rem); /* adjust if needed */
}

/* The content area (everything above the bar) */
#passage-UI-NPC-Sex .npcsex-root,
#passage-UI-NPC-Sex #npcsex-text,
#passage-UI-NPC-Sex .passage{
  min-height:0; /* important for flex children */
}

/* Action bar sits after content and stays visible at bottom without overlaying */
#npcsex-actions-bar{
  position:sticky;
  bottom:0;
  z-index:10;

  /* make it occupy layout space instead of overlaying content */
  margin-top:auto;

  /* match your styling */
  padding:0.5rem 0;
  text-align:center;
  background:radial-gradient(
    circle at top,
    rgba(255, 255, 255, 0.05),
    rgba(0, 0, 0, 0.95)
  );
  border-top:1px solid rgba(255,255,255,0.18);
  box-shadow:
    0 -2px 18px rgba(0,0,0,0.85),
    0 0 32px rgba(120, 40, 200, 0.45);
  backdrop-filter:blur(6px);
}

/* Desktop PC: fix action bar to bottom of viewport */
@media (min-width: 901px){
  #npcsex-actions-bar{
    position:fixed;
    bottom:0;

    /* align with story column */
    left:21rem;
    right:1.5rem;

    margin-top:0;
  }

  /* prevent content being hidden behind fixed bar */
  #story{
    padding-bottom:6rem;
  }
}

/* Mobile: keep current sticky behavior + margins */
@media (max-width:900px){
  #npcsex-actions-bar{
    position:sticky;
    bottom:0;
    left:1rem;
    right:1rem;
  }
}

/* Landscape: keep the same alignment (no “empty space” stretching look) */
@media (orientation:landscape) and (max-height:500px) and (min-width:901px){
  #npcsex-actions-bar{
    left:21rem;
    right:1.5rem;
  }
}

@media (orientation:landscape) and (max-height:500px) and (max-width:900px){
  #npcsex-actions-bar{
    left:1rem;
    right:1rem;
  }
}

/* Ensure the bar width matches your story column automatically */
/* Temporailiy removed */
/* #npcsex-actions-bar{ */
/*  left:auto; */
/*  right:auto; */
/* } */

@media (max-width:900px){
  #npcsex-actions-bar{
    left:1rem;
    right:1rem;
  }
}

/* Centered inner panel */
.npcsex-actions-inner{
  max-width:1100px;
  margin:0 auto;
  padding:0.5rem 1rem;
  display:flex;
  gap:0.75rem;
  align-items:center;
  justify-content:space-between;
  border-radius:999px;
  background:rgba(15, 15, 20, 0.95);
  box-shadow:0 0 12px rgba(0, 0, 0, 0.75);
}

/* Each group (Position / Foreplay / etc.) */
.npcsex-group{
  display:flex;
  flex-direction:column;
  gap:0.25rem;
  min-width:0;
}

/* Optional: keep help a bit narrower */
.npcsex-group.npcsex-side{
  min-width:110px;
  text-align:right;
}

/* Group titles */
.npcsex-group-title{
  font-size:0.75rem;
  text-transform:uppercase;
  letter-spacing:0.08em;
  opacity:0.75;
  line-height:1;
  color:rgba(220,210,255,0.88);
  text-shadow:0 0 6px rgba(150,120,255,0.7);
}

/* Allow button lists inside to wrap nicely */
#npcsex-actions-bar span,
#npcsex-actions-bar div{
  white-space:normal;
}

/* Keep buttons/links tidy inside bar */
#npcsex-actions-bar a.link-internal,
#npcsex-actions-bar button{
  margin:0.1rem 0.15rem;
}

/* Simple responsive tweak: stack into two rows on small screens */
@media (max-width:900px){
  .npcsex-actions-inner{
    flex-wrap:wrap;
    row-gap:0.5rem;
    border-radius:1rem;
    justify-content:center;
  }
}

/* Landscape: limit bar height and allow scrolling inside it */
@media (orientation:landscape) and (max-height:500px){
  /* reserve less space because the bar is capped + scrollable */
  .npcsex-root{
    padding-bottom:calc(5rem + env(safe-area-inset-bottom, 0px));
  }

   #passage-UI-NPC-Sex{
    padding-bottom:calc(5rem + env(safe-area-inset-bottom, 0px));
  }
  #passage-UI-NPC-Sex .npcsex-root{
    padding-bottom:calc(5rem + env(safe-area-inset-bottom, 0px));
  }

  #npcsex-actions-bar{
    padding:0.25rem 0;
    max-height:42vh;
    overflow-y:auto;
    -webkit-overflow-scrolling:touch;
  }

  .npcsex-actions-inner{
    padding:0.35rem 0.6rem;
    border-radius:1rem;
    gap:0.4rem;
    flex-wrap:wrap;
    justify-content:center;
  }

  .npcsex-group-title{
    font-size:0.68rem;
  }

  #npcsex-actions-bar a.link-internal,
  #npcsex-actions-bar button{
    margin:0.05rem 0.1rem;
  }

  /* mobile landscape */
  @media (max-width: 900px){
    #npcsex-actions-bar{
      left:1rem;
      right:1rem;
      width:auto;
    }
  }
}

/* ========== Status / meters ========== */

/* Tighten horizontal rules globally or scope to .npcsex-root */
.passage hr{
  margin:0.75rem 0;
  border:0;
  border-top:2px solid #666;
}

/* Optional: ensure no extra vertical gaps in this UI */
.npcsex-root .intro,
.npcsex-root #log,
.npcsex-root .status-row{
  margin:0;
  padding:0;
}

/* compact status row */
.status-row{
  display:flex;
  align-items:center;
  gap:.75rem;
  flex-wrap:wrap;
  margin:.25em 0;
}

/* pill for the stage */
.stage-chip{
  display:inline-flex;
  align-items:center;
  gap:.5em;
  padding:.25em .6em;
  border-radius:999px;
  border:1px solid rgba(255,255,255,.25);
  background:rgba(255,255,255,.06);
  font-weight:600;
}

/* arousal/affection progress inline with its % */
.meter{
  display:flex;
  align-items:center;
  gap:.5em;
  min-width:240px;
}

.meter progress{
  width:180px;
  height:10px;
}

/* percentage alignment */
#aff-pct{
  min-width:3ch;
  text-align:right;
}

/* nicer progress look (keeps colors generic) */
progress{
  -webkit-appearance:none;
  appearance:none;
  border:none;
  background:transparent;
}

progress::-webkit-progress-bar{
  background:rgba(255,255,255,.12);
  border-radius:8px;
}

progress::-webkit-progress-value{
  background:rgba(255,255,255,.6);
  border-radius:8px;
}

progress::-moz-progress-bar{
  background:rgba(255,255,255,.6);
  border-radius:8px;
}

/* ========== Glitch / glitchy text ========== */

.glitch,
.glitchy{
  color:white;
  position:relative;
}

/* legacy clip-based glitch */
.glitch::before{
  content:attr(data-text);
  width:calc(100% + 0.5em);
  position:absolute;
  left:-9px;
  text-shadow:1px 0 blue;
  top:-5px;
  color:#FFD;
  background:#111;
  overflow:hidden;
  clip:rect(0, 900px, 0, 0);
  animation:noise-anim-2 3s infinite linear alternate-reverse;
}

.glitch::after{
  content:attr(data-text);
  width:calc(100% + 0.5em);
  position:absolute;
  left:-5px;
  text-shadow:-1px 0 red;
  top:-5px;
  color:#DFF;
  background:#111;
  overflow:hidden;
  clip:rect(0, 900px, 0, 0);
  animation:noise-anim 2s infinite linear alternate-reverse;
}

/* newer clip-path based glitch */
.glitchy::before{
  content:attr(data-text);
  width:calc(100% + 0.5em);
  position:absolute;
  left:-9px;
  text-shadow:1px 0 blue;
  top:-5px;
  color:#FFD;
  background:#111;
  overflow:hidden;
  clip-path:polygon(0 0);
  animation:noise-animY-2 3s infinite linear alternate-reverse;
}

.glitchy::after{
  content:attr(data-text);
  width:calc(100% + 0.5em);
  position:absolute;
  left:-5px;
  text-shadow:-1px 0 red;
  top:-5px;
  color:#DFF;
  background:#111;
  overflow:hidden;
  clip-path:polygon(0 0);
  animation:noise-animY 2s infinite linear alternate-reverse;
}

/* keyframes (unchanged, just grouped) */
@keyframes noise-anim{
  0%   { clip: rect(80px, 9999px, 16px, 0); }
  5%   { clip: rect(94px, 9999px, 17px, 0); }
  10%  { clip: rect(9px, 9999px, 92px, 0); }
  15%  { clip: rect(37px, 9999px, 4px, 0); }
  20%  { clip: rect(53px, 9999px, 47px, 0); }
  25%  { clip: rect(99px, 9999px, 69px, 0); }
  30%  { clip: rect(15px, 9999px, 49px, 0); }
  35%  { clip: rect(83px, 9999px, 81px, 0); }
  40%  { clip: rect(55px, 9999px, 43px, 0); }
  45%  { clip: rect(28px, 9999px, 27px, 0); }
  50%  { clip: rect(64px, 9999px, 28px, 0); }
  55%  { clip: rect(76px, 9999px, 91px, 0); }
  60%  { clip: rect(29px, 9999px, 72px, 0); }
  65%  { clip: rect(70px, 9999px, 10px, 0); }
  70%  { clip: rect(5px, 9999px, 84px, 0); }
  75%  { clip: rect(46px, 9999px, 81px, 0); }
  80%  { clip: rect(7px, 9999px, 85px, 0); }
  85%  { clip: rect(17px, 9999px, 89px, 0); }
  90%  { clip: rect(41px, 9999px, 59px, 0); }
  95%  { clip: rect(53px, 9999px, 90px, 0); }
  100% { clip: rect(10px, 9999px, 30px, 0); }
}

@keyframes noise-anim-2{
  0%   { clip: rect(84px, 9999px, 3px, 0); }
  5%   { clip: rect(31px, 9999px, 69px, 0); }
  10%  { clip: rect(37px, 9999px, 33px, 0); }
  15%  { clip: rect(18px, 9999px, 6px, 0); }
  20%  { clip: rect(25px, 9999px, 87px, 0); }
  25%  { clip: rect(100px, 9999px, 8px, 0); }
  30%  { clip: rect(24px, 9999px, 87px, 0); }
  35%  { clip: rect(39px, 9999px, 16px, 0); }
  40%  { clip: rect(96px, 9999px, 25px, 0); }
  45%  { clip: rect(16px, 9999px, 100px, 0); }
  50%  { clip: rect(92px, 9999px, 76px, 0); }
  55%  { clip: rect(29px, 9999px, 40px, 0); }
  60%  { clip: rect(40px, 9999px, 39px, 0); }
  65%  { clip: rect(94px, 9999px, 44px, 0); }
